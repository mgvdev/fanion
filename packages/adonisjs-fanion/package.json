{
  "name": "@mgvdev/fanion-adonisjs",
  "description": "AdonisJS adapter for Fanion feature flagging library",
  "version": "0.1.2",
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/providers",
    "build/stubs",
    "build/index.d.ts",
    "build/index.js",
    "build/configure.d.ts",
    "build/configure.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./fanion_provider": "./build/providers/fanion_provider.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "pnpm run lint",
    "test": "c8 pnpm run quick:test",
    "prebuild": "pnpm run lint && pnpm run clean",
    "build": "tsc",
    "postbuild": "pnpm run copy:templates",
    "release": "pnpm run build && pnpm release-it",
    "version": "pnpm run build",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "adonisjs",
    "feature-flags",
    "feature-toggles",
    "fanion",
    "flags",
    "toggles"
  ],
  "author": "Maxence Guyonvarho <contact@mgvdev.io>",
  "license": "MIT",
  "dependencies": {
    "fanion": "^0.6.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.12.0",
    "@adonisjs/eslint-config": "2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.15.0",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0",
    "knex": "^3.1.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "yarn": false,
    "npmInstall": false,
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
