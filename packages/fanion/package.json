{
  "name": "fanion",
  "type": "module",
  "version": "1.0.0",
  "description": "Feature flagging library for Node.js",
  "keywords": [
    "feature",
    "flag",
    "feature-flag",
    "feature-toggle",
    "feature-flags",
    "flag",
    "toggle"
  ],
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js"
  },
  "files": [
    "build"
